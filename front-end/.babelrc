{
  "presets": [
    ["@babel/preset-env", {"modules": false}],
    "@babel/react",
    "@babel/flow"
  ],
  "plugins": [
    ["module-resolver", {
      "root": ["./src"],
      "alias": {
        "@": "./src"
      }
    },
  ],
  "inline-react-svg",
  "babel-plugin-styled-components",
  "@babel/plugin-syntax-dynamic-import"
  ],
  "env": {
    "test": {
      "presets": [
        "@babel/preset-env",
        "@babel/react",
        "@babel/flow"
      ],
      "plugins": [
        ["module-resolver", {
          "root": ["./src"],
          "alias": {
            "@": "./src"
          }
        },
      ],
      "inline-react-svg",
      "babel-plugin-styled-components",
      "@babel/plugin-syntax-dynamic-import"
      ]
    }
  }
}
